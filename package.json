		"typescript": "^5.5.3",
		"typescript-eslint": "8.0.0-alpha.47",
		"vite": "^5.3.4",
		"zod": "^3.23.8"
	},
	"type": "module",
	"dependencies": {
		"@supabase/ssr": "^0.4.0",
		"@supabase/supabase-js": "^2.44.4",
		"dotenv": "^16.4.5"
	}
