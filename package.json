		"@sveltejs/vite-plugin-svelte": "^3.1.1",
		"@types/eslint": "^8.56.10",
		"autoprefixer": "^10.4.19",
		"eslint": "^9.7.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-svelte": "^2.43.0",
		"flowbite": "^2.4.1",
		"flowbite-svelte": "^0.46.15",
		"flowbite-svelte-icons": "^1.6.1",
